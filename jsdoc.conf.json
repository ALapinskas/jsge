{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./src", "./src/base"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
    },
    "opts": {
        "destination": "./docs"  
    }
}

