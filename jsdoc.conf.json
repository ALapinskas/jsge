{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./src", "./src/base"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "default": {
            "staticFiles": {
                "include": [
                    "./schemes/",
                    "./docs/images"
                ]
            }
        }
    },
    "opts": {
        "destination": "./docs/build",
        "tutorials": "./docs/tutorials"
    }
}

